<h1>Outdoor.sy</h1>
<%= form_with url: upload_file_home_index_path, local: false,multipart: true, format: :js do |form| %>
  <div class="form-group">
    <%= form.label :file %>
    <%= form.file_field :file, required: true, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= form.submit 'Upload', class: 'btn btn-primary' %>
  </div>
<% end %>

<div id="vehicle-table">
  <%= render partial: 'vehicles/table', locals: { vehicles: @vehicles } %>
</div>

